.clocr-title { color:#101110;}
.color-pricing { color:#807F79;}
.bacolor { background-color:#008AE1;}
.bgcolor-title { background-color:#F3F2F0;}
.bgcolor-price { background-color:#EDF5FF;}

.head-information { position:relative;background-color:#4a91e3;height:400px;}
.information {position:absolute;top:0;left:0;z-index:111;text-align:center;width:100%;height:100%; }
.information div{color:#FEFEFF;margin-bottom:30px;line-height:1.6;text-shadow: 2px 2px 3px #333;}
/*产品定价*/
.service-pricing { height:650px;background-color:#F3F2F0;margin-top:-3px;}
.pricing-details { text-align:center;width:800px;margin:auto;margin-top:50px;}
.pricing-details ul { background-color:#fff;width:250px;height:475px;float:left;}
.pricing-details ul li:first-child{margin-bottom: 20px;padding-top:30px;font-size:22px;color:#61605B;}
.pricing-details ul .btn-click-login{background-color:#fff !important;color:#008AE1 !important;}
.pricing-details ul .btn-click-login a{color:#4a98e7 !important;}
.pricing-details ul .btn-click{ border: 1px solid #008AE1;padding: 5px 10px; width: 100px;
                                background-color: #008AE1;margin: 15px auto;border-radius: 3px;cursor:pointer;}
.pricing-details ul .login-trial a{color:#fff;}
.pricing-details ul .login-trial:hover{background-color: #1C79B4;}
.pricing-details ul .line-pricing{ height: 2px;border-bottom: 1px dashed #ccc;margin: 20px auto 30px auto;width:200px;}
.pricing-details ul .details-function{font-size:14px;color:#A3A2A0;text-align:left;margin:15px 0 0 25px;}
.pricing-details ul .details-function a{color:#0A0B0A}
/*价格表*/
.price-list table { width:800px;margin:auto;margin-bottom:20px;border-collapse:collapse;}
.price-list table td{border:1px solid #ccc;border-style:dashed;text-align:center;height:40px;border-bottom:0px;
                     border-right:0px;position:relative;}
table td span{position:absolute;color: #1eb452;right:20px;}
.price-list table tr td:first-child{width:148px;}
.price-list table tr td:last-child{border-right:1px dashed #ccc;}
.price-list table tr:last-child td{border-bottom:1px dashed #ccc;}
.price-list table .use-number {border: 1px solid #0F8EE1;padding: 2px 10px;color: #fff;background-color: #0F8EE1;
                               border-radius: 25px;width: 55px;margin: auto; }
.price-list .reminder { max-width:800px;margin:auto;}
.price-list .price-line { height: 1px;border-bottom: 1px solid #ccc;width: 850px;margin: 30px auto;}
/*费用估算*/
.estimate .estimate-price {width:800px;margin:auto;margin-bottom:20px;height:300px; }
.estimate-price ul li { width:400px;margin-bottom:20px;}
.estimate .estimate-price .use-line,.time-line,.discount-line { width: 300px;height: 15px;border: 1px solid #ccc;border-radius: 15px;
                                                              float: left;margin-right:30px;position:relative;cursor:pointer;}
.estimate-price .use-line div {background-color: #008AE1;width: 55%;height: 15px;border-radius: 15px;}
.estimate-price .use-line .btn-number {position: absolute;width: 10px;top: -4px;background-color: #fff;margin-left: 53%;height: 20px;
                                       border: 1px solid #ccc;cursor:pointer;}
.estimate-price ul { float:left}
.estimate-price ul input{width:30px;height:15px;line-height:17px;}
.estimate-price li div:first-child { color:#0A0B0A;margin-bottom:10px;}
.round { width: 20px;height: 20px;display: inline-block;background-color: #fff;border: solid 1px #4a98e7;text-align: center;
         border-radius: 11px;cursor: pointer;box-shadow: 0 0 1px #4a98e7 inset;line-height: 22px !important;}
.hover span{background-color: #4a98e7;height: 12px;width: 12px; border-radius: 6px;margin-top: 4px;}
.estimate-price .cost { margin-left:450px;width:300px;}
.estimate-price .cost div{font-size:18px;margin-bottom:30px;line-height:22px;}
.cost .check-box { width:20px;height:20px;border:1px solid #ccc;float:left;margin-right:20px;color:#fff;cursor:pointer}
.cost .check-box.hover{background-color:#4a98e7;}
.cost div span { font-size:17px;}
.estimate-price .cost .count { border: 0;border-top: 3px solid #ccc;width: 300px;color: #0689E0;
                               padding-top: 10px;font-size: 18px;}
.cost .count span { color:#0689E0;}
/*技术支持*/
.technical-support { height:500px;background-color:#F3F2F0;}
.technical-support .details{width:850px;margin:auto;height:400px;}
.technical-support ul{width:250px;background-color:#008AE1;height:300px;margin:auto;float:left;margin-top:100px;}
.technical-support ul li{color:#FBFBFC;text-align:center;margin:20px 20px 30px 20px;}
.technical-support ul li a{color:#FBFBFC;text-align:center;margin:20px 20px 30px 20px;}
